{
  "rules": {
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "quotes": [
      2,
      "single"
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "no-console": 1,
    "prefer-const": 1,
    "no-var": 1,
    "prefer-arrow-callback": 1,
    "prefer-spread": 1,
    "no-unused-vars": 1,
    "no-const-assign": 2,
    "no-class-assign": 2,
    "no-dupe-class-members": 2
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true
    }
  },
  "extends": [
    "eslint:recommended"
  ]
}